{
    "name": "@sleuren/sleuren-js",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "version": "1.0.0",
    "description": "Official sleuren SDK for JavaScript",
    "homepage": "https://sleuren.com",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/sleuren/js-client.git"
    },
    "bundlesize": [
